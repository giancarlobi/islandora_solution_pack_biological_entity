                    <delete>
                      <path>self::node()</path>
                      <context>self</context>
                    </delete>
                  </actions>
                </properties>
                <children/>
              </element>
              <element name="kingdom">
                <properties>
                  <type>textfield</type>
                  <access>TRUE</access>
                  <default_value>Plantae</default_value>
