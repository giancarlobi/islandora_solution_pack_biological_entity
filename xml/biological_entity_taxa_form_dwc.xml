              <element name="order">
                <properties>
                  <type>textfield</type>
                  <access>TRUE</access>
                  <description>The full scientific name of the order in which the taxon is classified.</description>
                  <required>FALSE</required>
                  <title>Order</title>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>/dwr:SimpleDarwinRecordSet/dwr:SimpleDarwinRecord</path>
                      <context>document</context>
                      <schema/>
                      <type>element</type>
                      <prefix>dwc</prefix>
                      <value>dwc:order</value>
