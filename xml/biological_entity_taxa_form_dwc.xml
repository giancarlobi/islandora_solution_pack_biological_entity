                  <type>textarea</type>
                  <access>TRUE</access>
                  <description>Comments or notes about the taxon or name.</description>
                  <required>FALSE</required>
                  <title>Taxon Remarks</title>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>/dwr:SimpleDarwinRecordSet/dwr:SimpleDarwinRecord</path>
                      <context>document</context>
