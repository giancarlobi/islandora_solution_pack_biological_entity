                </properties>
                <children/>
              </element>
            </children>
          </element>
         <element name="Submit">
            <properties>
              <type>submit</type>
              <access>TRUE</access>
              <default_value>Submit</default_value>
              <required>FALSE</required>
              <title>Submit</title>
              <tree>TRUE</tree>
            </properties>
            <children/>
          </element>
        </children>
      </element>
    </children>
  </form>
</definition>
