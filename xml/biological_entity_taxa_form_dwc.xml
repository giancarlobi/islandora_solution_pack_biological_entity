                <properties>
                  <type>textfield</type>
                  <access>TRUE</access>
                  <description>A common or vernacular name.</description>
                  <required>FALSE</required>
                  <title>Vernacular Name</title>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>/dwr:SimpleDarwinRecordSet/dwr:SimpleDarwinRecord</path>
