              <element name="family">
                <properties>
                  <type>textfield</type>
                  <access>TRUE</access>
                  <description>The full scientific name of the family in which the taxon is classified.</description>
                  <required>FALSE</required>
                  <title>Family</title>
                  <tree>TRUE</tree>
                  <actions>
                    <create>
                      <path>/dwr:SimpleDarwinRecordSet/dwr:SimpleDarwinRecord</path>
                      <context>document</context>
                      <schema/>
                      <type>element</type>
                      <prefix>dwc</prefix>
